<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<title>@ViewBag.Title</title>
	<style>
		table {
			/*width: 30px;*/
			border: 1px solid black;
		}

			table td > div {
				overflow: hidden;
				height: 5px;
				width: 5px;
			}

			.alive {
				background-color: green;
			}
	</style>
</head>
<body>
	<div id="header"></div>
	<div id="body">@RenderBody()</div>
	<footer></footer>

	@RenderSection("scripts", required: false)
</body>
</html>